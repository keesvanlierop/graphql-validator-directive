{
  "name": "@k2s/graphql-validator-directive",
  "version": "0.0.1",
  "description": "GraphQL directive using validator package to validate input",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags",
    "build": "tsc",
    "format": "yarn lint 'src/**/*.ts' --quiet --fix",
    "lint": "eslint"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "graphql",
    "validator",
    "directive",
    "constraint"
  ],
  "author": "Kees van Lierop",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/keesvanlierop/graphql-validator-directive"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "validator": "^13.1.1"
  },
  "dependencies": {
    "apollo-server-errors": "^2.4.1",
    "graphql": "^15.1.0",
    "graphql-any-type": "^0.0.3",
    "graphql-tools": "^6.0.10"
  }
}
